			label: Typ adresy
		first_name:
			label: 'Křestní jméno:'
			placeholder: křestní jméno
		last_name:
			label: 'Příjmení:'
			placeholder: příjmení
		phone_number:
			label: 'Telefon:'
			placeholder: například 605 123 456
		address:
			label: 'Ulice:'
			placeholder: napište ulici
		number:
			label: 'Číslo domu:'
			placeholder: například 14/A
		zip:
			label: 'PSČ:'
			placeholder: například 10100
		city:
			label: 'Mesto:'
			placeholder: například Praha
		country:
			label: 'Země:'
